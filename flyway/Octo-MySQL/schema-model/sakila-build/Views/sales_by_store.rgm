view "sakila-build"."sales_by_store" {
  text = select concat(`c`.`city`,',',`cy`.`country`) AS `store`,concat(`m`.`first_name`,' ',`m`.`last_name`) AS `manager`,sum(`p`.`amount`) AS `total_sales` from (((((((`sakila-build`.`payment` `p` join `sakila-build`.`rental` `r` on((`p`.`rental_id` = `r`.`rental_id`))) join `sakila-build`.`inventory` `i` on((`r`.`inventory_id` = `i`.`inventory_id`))) join `sakila-build`.`store` `s` on((`i`.`store_id` = `s`.`store_id`))) join `sakila-build`.`address` `a` on((`s`.`address_id` = `a`.`address_id`))) join `sakila-build`.`city` `c` on((`a`.`city_id` = `c`.`city_id`))) join `sakila-build`.`country` `cy` on((`c`.`country_id` = `cy`.`country_id`))) join `sakila-build`.`staff` `m` on((`s`.`manager_staff_id` = `m`.`staff_id`))) group by `s`.`store_id` order by `cy`.`country`,`c`.`city`
}

