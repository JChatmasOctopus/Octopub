view "sakila-prod-02"."sales_by_store" {
  text = select concat(`c`.`city`,',',`cy`.`country`) AS `store`,concat(`m`.`first_name`,' ',`m`.`last_name`) AS `manager`,sum(`p`.`amount`) AS `total_sales` from (((((((`sakila-prod-02`.`payment` `p` join `sakila-prod-02`.`rental` `r` on((`p`.`rental_id` = `r`.`rental_id`))) join `sakila-prod-02`.`inventory` `i` on((`r`.`inventory_id` = `i`.`inventory_id`))) join `sakila-prod-02`.`store` `s` on((`i`.`store_id` = `s`.`store_id`))) join `sakila-prod-02`.`address` `a` on((`s`.`address_id` = `a`.`address_id`))) join `sakila-prod-02`.`city` `c` on((`a`.`city_id` = `c`.`city_id`))) join `sakila-prod-02`.`country` `cy` on((`c`.`country_id` = `cy`.`country_id`))) join `sakila-prod-02`.`staff` `m` on((`s`.`manager_staff_id` = `m`.`staff_id`))) group by `s`.`store_id` order by `cy`.`country`,`c`.`city`
}

